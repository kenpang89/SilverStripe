{
    "name": "silverleague/logviewer",
    "description": "View your SilverStripe logs from inside the CMS",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/silverleague/silverstripe-logviewer",
    "license": "MIT",
    "keywords": ["silverstripe", "error", "cms", "log", "monolog"],
    "authors": [
        {
            "name": "Robbie Averill",
            "email": "robbie@silverstripe.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "silverstripe/admin": "^1.0@dev",
        "silverstripe/framework": "^4.0@dev",
        "monolog/monolog": "~1.11",
        "silverstripe/crontask": "2.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverLeague\\LogViewer\\": "src/",
            "SilverLeague\\LogViewer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
